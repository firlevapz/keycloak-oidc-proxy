# OIDC Proxy Configuration
# Copy this file to .env and modify the values as needed

# Required: OIDC well-known configuration URL
# Example: https://auth.hoad.at/application/o/audiobookshelf/.well-known/openid-configuration
OIDC_CONFIGURATION_URL=https://your-oidc-provider.com/.well-known/openid-configuration

# Optional: Log level (trace, debug, info, warn, error, fatal, panic)
# Default: info
LOG_LEVEL=info

# Optional: Port to listen on
# Default: 8080
PORT=8080
